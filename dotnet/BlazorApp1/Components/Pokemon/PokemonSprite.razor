<h3>PokemonSprite</h3>

<img src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/official-artwork/@(DexId).png"
     alt="Pokemon @(DexId) sprite"
     width="300"
     height="300"
     style="image-rendering: pixelated"/>

@code {
    [Parameter] public required int DexId { get; set; }
}